<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bin135.augscan">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />


    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        tools:replace="android:appComponentFactory"'
        <activity android:name="com.example.bin135.augscan.ScanCodeActivitysearch"></activity>
        <activity android:name="com.example.bin135.augscan.ScanCodeActivity" />
        <activity android:name="com.example.bin135.augscan.ScanCodeActivitydel" />
        <activity android:name="com.example.bin135.augscan.viewInventoryActivity" />
        <activity android:name="com.example.bin135.augscan.scanItemsActivity" />
        <activity android:name="com.example.bin135.augscan.deleteItemsActivity" />
        <activity android:name="com.example.bin135.augscan.additemActivity" />
        <activity android:name="com.example.bin135.augscan.dashboardActivity" />
        <activity android:name="com.example.bin135.augscan.RegisterActivity" />
        <activity android:name="com.example.bin135.augscan.LoginActivity" />
        <activity android:name="com.example.bin135.augscan.MainActivity" />
        <activity
            android:name="com.example.bin135.augscan.SplashsActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>